{
  "name": "game-of-worlds",
  "version": "2017.11.19",
  "description": "Game of Worlds App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "win": "build --win"
  },
  "repository": "https://github.com/matejmosko/gow",
  "keywords": [
    "Game of Worlds",
    "Board game",
    "Game",
    "Pubgame"
  ],
  "author": "Matej Mo≈°ko",
  "license": "CC0-1.0",
  "postinstall": "install-app-deps",
  "devDependencies": {
    "electron": "^1.7.5",
    "electron-builder": "^18.0.1"
  },
  "dependencies": {
    "electron-settings": "^3.1.1",
    "fs": "0.0.1-security",
    "jquery": "^3.1.1",
    "jsdom": "^9.11.0",
    "mousetrap": "^1.6.1",
    "mustache": "^2.3.0",
    "nedb": "^1.8.0",
    "xel": "0.0.48"
  },
  "build": {
    "productName": "Game of Worlds Admin",
    "appId": "com.electron.MatejMosko.GameofWorlds",
